{
  "kind": "collectionType",
  "collectionName": "sites",
  "info": {
    "singularName": "site",
    "pluralName": "sites",
    "displayName": "Site",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "WGS84_lat": {
      "type": "decimal",
      "required": true
    },
    "WGS84_lon": {
      "type": "decimal",
      "required": true
    },
    "provider": {
      "type": "enumeration",
      "enum": ["ugz_intern", "innet", "meteoblue"]
    },
    "active": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "loggers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::logger.logger",
      "mappedBy": "site"
    }
  }
}
